                <li class="list-group-item">Propiedades Vendidas: @Model.SoldProperties</li>
                <li class="list-group-item">Agentes Activos: @Model.ActiveAgents</li>
                <li class="list-group-item">Agentes Inactivos: @Model.InactiveAgents</li>
                <li class="list-group-item">Clientes Activos: @Model.ActiveClients</li>
                <li class="list-group-item">Clientes Inactivos: @Model.InactiveClients</li>
                <li class="list-group-item">Desarrolladores Activos: @Model.ActiveDevelopers</li>
